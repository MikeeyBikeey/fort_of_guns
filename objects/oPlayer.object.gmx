if keyboard_check(ord("A")) {
    scr_player_walk(-1, 0);
}
if keyboard_check(ord("D")) {
    scr_player_walk(1, 0);
}
if keyboard_check(ord("W")) {
    scr_player_walk(0, -1);
}
if keyboard_check(ord("S")) {
    scr_player_walk(0, 1);
}

if real_dir != -1 {
    if spd == 0 {
        direction = real_dir;
        if spd &lt; max_spd {
            spd = clamp(spd + acc, 0, max_spd);
        }
    } else if abs(angle_difference(real_dir, direction)) &gt; 100 { // 135
        spd = max(spd - acc, 0);
    } else {
        direction -= angle_difference(direction, real_dir) / ((spd / max_spd) * turn_divid);
        if spd &lt; max_spd {
            spd = clamp(spd + acc, 0, max_spd);
        }
    }
}

