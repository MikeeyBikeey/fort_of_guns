            <string>///draw ammo

var gun = xGunGet(lgun);
if instance_exists(gun) &amp;&amp; instance_exists(lhandgun) {
    if gun.endless_clip {
        draw_sprite(sAmmoEndless, 0, 1, 1);
    } else {
        var num_bullets = 0;
        for(var i = 0; i &lt; ceil(gun.mag_size / 10); i++) {
            for(var j = 0; j &lt; 10; j++) {
                num_bullets += 1;
                if num_bullets &gt; gun.mag_size {
                    break;
                }
                
                draw_sprite(
                    sAmmoBullet,
                    num_bullets &gt; lhandgun.bullets,
                    1 + j * (sprite_get_width(sAmmoBullet) + 1),
                    1 + i * (sprite_get_height(sAmmoBullet) + 1),
                );
            }
        }
    }
}

