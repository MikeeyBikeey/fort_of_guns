          <argument>
            <kind>1</kind>
            <string>///draw gun and body

var gun_dir = point_direction(x, y, mouse_x, mouse_y);

var yscale = 1;
if gun_dir &gt; 90 &amp;&amp; gun_dir &lt; 270 {
    yscale = -1;
}

draw_sprite_ext(
    sPlayerUpperBody,
    1,
    x,
    y,
    abs(image_xscale),
    image_yscale * yscale,
    gun_dir,
    image_blend,
    image_alpha,
);

// draw punch
if !punching {
    xHandGunDraw(lhandgun);
} else {
    var dist;
    if alarm[3] != -1 {
        dist = (punch_out_time - alarm[3]) / punch_out_time * punch_dist;
    } else {
        dist = alarm[4] / punch_in_time * punch_dist;
    }
    
    var xx = x + lengthdir_x(dist, gun_dir);
    var yy = y + lengthdir_y(dist, gun_dir);
    
    with lhandgun {
        var gun = xGunGet(gun_id);
        if instance_exists(gun) {
            draw_sprite_ext(gun.sprite, image_index, xx, yy, image_xscale, image_yscale, image_angle, image_blend, image_alpha); 
        }
    }
}

draw_sprite_ext(
    sPlayerUpperBody,
    0,
    x,
    y,
    abs(image_xscale),
    image_yscale * yscale,
    gun_dir,
    image_blend,
    image_alpha,
);

